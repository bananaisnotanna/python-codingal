n=int(input("enter num: "))
originalnum=n
dn=len(str(n))
sum=0
while n>0:
    rem=n%10
    n=n//10
    sum=sum+ (rem**dn)
    print(rem)
if sum==originalnum:
    print(originalnum,"is an armstrong number.")
else:
    print(originalnum,"is not an armstrong number.")



n=int(input("enter num: "))
sumofsquares=0

while n>0:
    rem=n%10
    n=n//10
    sumofsquares=sumofsquares + (rem**2)
print(sumofsquares)
